{
  "$ref": "#/definitions/ConstitucionShallow",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "CommonData": {
      "additionalProperties": false,
      "properties": {
        "etiquetas": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "pagina": {
          "type": "number"
        },
        "sobre": {
          "type": "string"
        }
      },
      "required": [
        "pagina",
        "sobre",
        "etiquetas"
      ],
      "type": "object"
    },
    "ConstitucionShallow": {
      "additionalProperties": false,
      "properties": {
        "enlaces": {
          "items": {
            "$ref": "#/definitions/Enlace"
          },
          "type": "array"
        },
        "items": {
          "$ref": "#/definitions/Record%3Cstring%2CItemObject%3E"
        }
      },
      "required": [
        "items",
        "enlaces"
      ],
      "type": "object"
    },
    "Enlace": {
      "additionalProperties": false,
      "properties": {
        "autor": {
          "type": "string"
        },
        "desde": {
          "type": "string"
        },
        "etiqueta": {
          "type": "string"
        },
        "hacia": {
          "type": "string"
        },
        "texto": {
          "type": "string"
        }
      },
      "required": [
        "desde",
        "hacia"
      ],
      "type": "object"
    },
    "ItemObject": {
      "additionalProperties": false,
      "properties": {
        "content": {
          "type": "string"
        },
        "data": {
          "$ref": "#/definitions/CommonData"
        },
        "key": {
          "description": "Unique among the same with the same parent.",
          "type": [
            "number",
            "string"
          ]
        },
        "label": {
          "type": "string"
        },
        "level": {
          "type": "number"
        },
        "oid": {
          "type": "string"
        },
        "ordinal": {
          "type": "number"
        },
        "parent": {
          "description": "Parent oid",
          "type": "string"
        },
        "type": {
          "$ref": "#/definitions/ItemType"
        }
      },
      "required": [
        "oid",
        "type",
        "level"
      ],
      "type": "object"
    },
    "ItemType": {
      "enum": [
        "preambulo",
        "capitulo",
        "titulo",
        "articulo",
        "inciso",
        "transitorias",
        "transitoria"
      ],
      "type": "string"
    },
    "Record<string,ItemObject>": {
      "additionalProperties": {
        "$ref": "#/definitions/ItemObject"
      },
      "type": "object"
    }
  }
}