{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "build/main",
    "rootDir": "src",
    "lib": [
      "es2017"
    ],
    "types": [],
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
    },
  },
  "include": [
    "src/**/*.ts",
    "src/constitucion.*.json"
  ],
  "exclude": [
    "node_modules/**"
  ],
  "compileOnSave": false
}
